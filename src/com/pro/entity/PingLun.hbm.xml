<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pro.entity.PingLun" table="t_pinglun">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property access="field" column="t_musicid" length="200"
			name="musicid" />
		<property access="field" column="t_last" length="200"
			name="last" />
		<property access="field" column="t_musicname" length="200"
			name="musicname" />
		<property access="field" column="t_content" length="2000"
			name="content" />
		<property access="field" column="t_date" length="200" name="date" />
		<property access="field" column="t_pinlunid" length="200"
			name="pinlunid" />
		<property access="field" column="t_pinglunname" length="200"
			name="pinglunname" />
		<property access="field" column="t_img" length="200"
			name="img" />	
	</class>
</hibernate-mapping>