<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pro.entity.CollectMusic" table="t_collectmusic">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property access="field" column="t_stuid" length="200" name="stuid" />
		<property access="field" column="t_stuname" length="200"
			name="stuname" />
		<property access="field" column="t_musicid" length="200"
			name="musicid" />
		<property access="field" column="t_types" length="200" name="types" />
		<property access="field" column="t_musicname" length="200"
			name="musicname" />
		<property access="field" column="t_file" length="200" name="file" />
		<property access="field" column="t_content" length="2000"
			name="content" />
		<property access="field" column="t_img" length="2000"
			name="img" />
		<property access="field" column="t_ycz" length="2000"
			name="ycz" />
	</class>
</hibernate-mapping>