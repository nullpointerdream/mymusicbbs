<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.z.plugin.bbss.Message" table="t_message">
<id name="id" type="java.lang.Integer"><column name="id"/><generator class="native"/></id>
<property access="field" column="t_title" length="200" name="title"/>
<property access="field" column="t_content" length="2000" name="content"/>
<property access="field" column="t_userid" length="200" name="userid"/>
<property access="field" column="t_username" length="200" name="username"/>
<property access="field" column="t_datetime" length="200" name="datetime"/>
<property access="field" column="t_img" length="200" name="img"/>
<property access="field" column="t_last" length="200" name="last"/>
<property access="field" column="t_titleid" length="200" name="titleid"/>
</class>
</hibernate-mapping>